{
  "release_notes": {
    "commit_format": "* :hash: :message",
    "title": "Release :version"
  },
  "before_release": [
    "go build -ldflags='-w -s -X github.com/vite-cloud/vite/build.Version=:version -X github.com/vite-cloud/vite/globals.Version=:fullCommit' -gcflags=all='-l'"
  ],
  "after_release": [
    "rm vite"
  ],
  "assets": [
    "./vite"
  ],
  "tag_message": "Version :tag"
}